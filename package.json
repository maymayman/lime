  "dependencies": {
    "parse-server": "4.5.0"
  },
  "devDependencies": {
    "ts-node": "9.1.1",
    "typescript": "4.2.3",
    "@types/node": "14.14.32",
    "@types/parse": "2.18.3",
    "@types/express": "4.17.11",
    "nodemon": "2.0.7"
  }
